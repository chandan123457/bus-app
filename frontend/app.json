{
  "expo": {
    "name": "Go Gantabya",
    "slug": "bus-booking-app",
    "owner": "bottle1234",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E88E5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.busbooking.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#1E88E5"
      },
      "package": "com.busbooking.app"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "sdkVersion": "54.0.0",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to save tickets.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save tickets to your device.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-file-system"
    ],
    "extra": {
      "eas": {
        "projectId": "b320c74c-698d-4193-ac2c-46c0ed7db673"
      }
    }
  }
}
