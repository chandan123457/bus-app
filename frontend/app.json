{
  "expo": {
    "name": "Bus Booking",
    "slug": "bus-booking-app",
    "owner": "yuvi899",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E88E5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.busbooking.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#1E88E5"
      },
      "package": "com.busbooking.app"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "sdkVersion": "54.0.0",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b10c249a-0b5f-46d7-bdeb-bc6a90dd91d2"
      }
    }
  }
}
